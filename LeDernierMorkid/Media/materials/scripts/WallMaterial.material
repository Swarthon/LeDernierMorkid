// Basic Material
material WallMaterial 
{
    technique
    {
        pass WallMaterial
        {
            texture_unit 
            {
                texture rockwall.tga
                tex_address_mode wrap
                scale 1.0 1.0
                tex_coord_set 0
                colour_op modulate
            }
        }
    }
}

